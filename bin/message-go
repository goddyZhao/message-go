#!/usr/bin/env node

var program = require('commander');
var mgo = require('../');

program
  .version('0.0.1')
  .option('-f, --from [from]', 'Specify a single source file or directory including messages')
  .option('-t, --to [to]', 'Specify the message property file')
  .parse(process.argv);

mgo.go(program.from, program.to);
